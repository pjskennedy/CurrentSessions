
@font-face {
    font-family: 'Railway-thin';
    src: url(<%= asset_path 'raleway_thin-webfont.eot' %>);
    src: url(<%= asset_path 'raleway_thin-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'raleway_thin-webfont.ttf' %>) format('truetype'),
         url(<%= asset_path 'raleway_thin-webfont.svg' %>) format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'blake';
    src: url(<%= asset_path 'blake2.ttf' %>);
    font-weight: normal;
    font-style:normal;
}

@font-face {
    font-family: 'ProximaNovaLtRegular';
    src: url(<%= asset_path 'proximanova-light-webfont.eot' %>);
    src: url(<%= asset_path 'proximanova-light-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'proximanova-light-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'proximanova-light-webfont.ttf' %>) format('truetype');
    font-weight: normal;
    font-style: normal;
}